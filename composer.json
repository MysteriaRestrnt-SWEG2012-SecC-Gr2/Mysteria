{
    "require": {
        "craftcms/cms": "3.3.20.1",
        "phpmailer/phpmailer": "~6.1",
        "ramsey/uuid": "^3.9",
        "vlucas/phpdotenv": "^2.4.0"
    },
      "autoload": {
        "psr-4": {
            "Tutsplus\\Library\\": "src"
        }
      },
      "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
          "php": "7.0"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
      }
}
